{
  "name": "turahe/seotools",
  "description": "SEO Tools for Laravel and Lumen",
  "keywords": [
    "laravel",
    "lumen",
    "seo",
    "seotools",
    "webmaster",
    "metatags",
    "opengraph",
    "json-ld",
    "pwa",
    "manifest",
    "sitemap",
    "feed"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/turahe/seotools/issues",
    "source": "https://github.com/turahe/seotools"
  },
  "authors": [
    {
      "name": "Nur Wachid",
      "email": "nur@wach.id"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "illuminate/database": "^8.0|^9.0",
    "illuminate/support": "^8.0|^9.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.1",
    "orchestra/testbench": "^6.0",
    "phpspec/phpspec": "^7.0",
    "ext-dom": "*"
  },
  "autoload": {
    "psr-4": {
      "Turahe\\SEOTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Turahe\\SEOTools\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Turahe\\SEOTools\\Providers\\SEOToolsServiceProvider"
      ],
      "aliases": {
        "SEOMeta": "Turahe\\SEOTools\\Facades\\Meta",
        "OpenGraph": "Turahe\\SEOTools\\Facades\\OpenGraph",
        "Twitter": "Turahe\\SEOTools\\Facades\\TwitterCard",
        "JsonLd": "Turahe\\SEOTools\\Facades\\JsonLd",
        "SEO": "Turahe\\SEOTools\\Facades\\Tools",
        "PWA": "Turahe\\SEOTools\\Facades\\PWA"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
