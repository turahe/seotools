{
  "name": "turahe/metatags",
  "description": "Metatags for Laravel and Lumen",
  "keywords": [
    "laravel",
    "lumen",
    "seo",
    "Metatags",
    "webmaster",
    "metatags",
    "opengraph",
    "json-ld",
    "service worker"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/turahe/metatags/issues",
    "source": "https://github.com/turahe/metatags"
  },
  "authors": [
    {
      "name": "Nur Wachid",
      "email": "nur@wach.id"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "illuminate/config": "5.8.* || ^6.0 || ^7.0 || ^8.0",
    "illuminate/support": "5.8.* || ^6.0 || ^7.0 || ^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.8.4 || ^4.0 || ^5.0",
    "phpspec/phpspec": "~5.1.1 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "Turahe\\Metatags\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Turahe\\Metatags\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Turahe\\Metatags\\Providers\\MetatagsServiceProvider"
      ],
      "aliases": {
        "SEOMeta": "Turahe\\Metatags\\Facades\\SEOMeta",
        "OpenGraph": "Turahe\\Metatags\\Facades\\OpenGraph",
        "Twitter": "Turahe\\Metatags\\Facades\\TwitterCard",
        "JsonLd": "Turahe\\Metatags\\Facades\\JsonLd",
        "SEO": "Turahe\\Metatags\\Facades\\Metatags"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
